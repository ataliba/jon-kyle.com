{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "content": "enoki content",
    "start": "enoki content -w & watchify source/index.js -o bundles/bundle.js -t sheetify -p [ css-extract -o bundles/bundle.css ] source/index.js",
    "dev": "enoki content --watch & budo source/index.js --dir -s bundles/bundle.js -P -- -t sheetify -p [ css-extract -o bundles/bundle.css ]",
    "build": "enoki content && node scripts/build && browserify source/index.js -o bundles/bundle.js -t yo-yoify -t  [ sheetify -u sheetify-cssnext ] -g es2040 -p [ css-extract -o bundles/bundle.css ] -p tinyify"
  },
  "author": "Jon-Kyle Mohr",
  "license": "Apache-2.0",
  "devDependencies": {
    "browserify": "^15.2.0",
    "css-extract": "^1.3.0",
    "es2040": "^1.2.6",
    "sheetify": "^7.3.2",
    "sheetify-cssnext": "^1.0.7",
    "tinyify": "^2.4.0",
    "watchify": "^3.10.0",
    "yo-yoify": "^3.7.3"
  },
  "dependencies": {
    "bel": "^4.6.0",
    "choo": "^6.12.1",
    "create-html": "^3.1.0",
    "dayjs": "^1.6.10",
    "enoki": "^2.1.0",
    "escape-html": "^1.0.3",
    "ez.js": "^1.0.3",
    "fg-loadcss": "^2.0.1",
    "fontfaceobserver": "^2.0.9",
    "gr8": "^2.1.5",
    "intersection-observer": "^0.5.0",
    "join-path": "^1.1.1",
    "lilcss": "0.0.0",
    "markdown-it": "^8.4.0",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-for-inline": "^0.1.1",
    "markdown-it-html5-embed": "^0.4.0",
    "markdown-it-implicit-figures": "^0.9.0",
    "markdown-it-video": "^0.5.1",
    "marked": "^0.3.6",
    "medium-zoom": "^0.4.0",
    "monoimage": "^0.1.2",
    "nano-markdown": "^1.2.0",
    "nanocomponent": "^6.4.2",
    "nanomorph": "^4.0.4",
    "nanoraf": "^3.0.1",
    "object-keys": "^1.0.11",
    "object-values": "^1.0.0",
    "plyr": "^3.3.20",
    "recsst": "^1.1.2",
    "require-globify": "^1.4.1",
    "scroll-to": "0.0.2",
    "simplemde": "^1.11.2",
    "tags-input": "^1.1.1",
    "tweezer.js": "^1.4.0",
    "xhr": "^2.4.0",
    "xtend": "^4.0.1"
  }
}
